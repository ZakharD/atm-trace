{
  "name": "atm-trace",
  "version": "0.3.0",
  "description": "ATM Trace Service - a library to display binary data",
  "main": "lib/trace.js",
  "scripts": {
    "test": "eslint tests/** lib/** && ava"
  },
  "repository": "https://github.com/timgabets/atm-trace",
  "keywords": [
    "Logging",
    "Trace"
  ],
  "ava": {
    "files": [
      "tests/*.js"
    ],
    "source": [
      "lib/**/*.{js}"
    ],
    "concurrency": 5,
    "failFast": true,
    "failWithoutAssertions": false,
    "powerAssert": false
  },
  "author": {
    "name": "Tim Gabets",
    "email": "tim@gabets.ru",
    "url": "http://gabets.ru/"
  },
  "license": "LGPL-2.1",
  "devDependencies": {
    "eslint": "^4.12.0",
    "ava": "^0.24.0",
    "jsdom": "^11.5.1",
    "eslint-plugin-import": "^2.8.0"
  },
  "dependencies": {
    "atm-logging": "^0.1.4",
    "atm-timestamp": "^1.1.1"
 }
}
